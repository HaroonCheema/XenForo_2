  <template type="admin" title="avForumsTagEss_blacklist" version_id="2000001" version_string="2.0.0"><![CDATA[<xf:if is="$tag.exists()">
	<xf:title>{{ phrase('confirm_action') }}</xf:title>
<xf:else />
	<xf:title>{{ phrase('avForumsTagEss_add_tag_to_blacklist') }}</xf:title>
</xf:if>

<xf:form action="{{ link('tags/blacklist', null, {'existing_tag_id': $tag ? $tag.tag_id : 0}) }}" class="block" ajax="true">
	<div class="block-container">
		<div class="block-body">
			<xf:if is="$tag && $tag.exists()">
				<xf:inforow rowtype="confirm">
					{{ phrase('avForumsTagEss_please_confirm_that_you_want_to_blacklist_following_tag:') }}
					<strong><a href="{{ link('tags/edit', $tag) }}">{$tagName}</a></strong>
				</xf:inforow>
			</xf:if>
			<xf:textboxrow name="new_tag" label="{{ phrase('tag') }}" value="{{ $tagName }}" />
			<xf:checkboxrow>
				<xf:option name="isRegex" label="{{ phrase('avForumsTagEss_regex_blacklist') }}" value="1" selected="{$isRegex}" hint="{{ phrase('avForumsTagEss_regex_blacklist_explain') }}"/>
			</xf:checkboxrow>
		</div>

		<xf:submitrow rowtype="simple">
			<xf:html>
				<xf:button type="submit" name="test_gravatar" class="button--primary" value="1">
					<i class="fa fa-ban" aria-hidden="true"></i> {{ phrase('avForumsTagEss_blacklist') }}
				</xf:button>
			</xf:html>
		</xf:submitrow>
	</div>
</xf:form>]]></template>
  <template type="admin" title="avForumsTagEss_blacklist_delete" version_id="2000001" version_string="2.0.0"><![CDATA[<xf:title>{{ phrase('confirm_action') }}</xf:title>

<xf:form action="{{ link('tags/delete-blacklist', null, {'blacklist_id': $blacklist.blacklist_id}) }}" ajax="true" class="block">
	<div class="block-container">
		<div class="block-body">
			<xf:inforow rowtype="confirm">
				{{ phrase('avForums_please_confirm_that_you_want_to_delete_following_blacklisted_tag:') }}
				<strong>{$blacklist.tag}</strong>
			</xf:inforow>
		</div>
		<xf:submitrow rowtype="simple" icon="delete" />
	</div>
</xf:form>]]></template>
  <template type="admin" title="avForumsTagEss_blacklisted_tags" version_id="2010400" version_string="2.1.4"><![CDATA[<xf:title>{{ phrase('avForumsTagEss_blacklisted_tags') }}</xf:title>

<xf:pageaction>
	<xf:button href="{{ link('tags/blacklist') }}" icon="add">{{ phrase('avForumsTagEss_blacklist_tag') }}</xf:button>
</xf:pageaction>

<xf:form action="{{ link('tags/blacklisted') }}" class="block">
	<div class="block-outer">
		<span class="filterBlock">
			<xf:textbox name="containing" value="{$filters.containing}" placeholder="{{ phrase('tag_contains...') }}"
				class="input--inline filterBlock-input" />
			<span>
				<span class="filterBlock-label">{{ phrase('ordered_by:') }}</span>
				<xf:select name="order" value="{{ $filters.order ? $filters.order : 'tag' }}" class="input--inline filterBlock-input">
					<xf:option value="tag">{{ phrase('tag') }}</xf:option>
				</xf:select>
			</span>
			<xf:button type="submit">{{ phrase('go') }}</xf:button>
		</span>
	</div>
	
	<div class="block-container">
		<xf:if is="$blacklistedTags is not empty">
			<div class="block-body">
				<xf:datalist data-xf-init="responsive-data-list">
					<xf:datarow rowtype="header">
						<xf:cell class="dataList-cell--min"><xf:checkbox standalone="true"><xf:option data-match="input:checkbox[name='selected_tags[]']" check-all="< .block-container"  data-xf-init="tooltip" title="{{ phrase('select_all') }}" /></xf:checkbox></xf:cell>
						<xf:cell class="dataList-cell--min">{{ phrase('avForumsTagEss_regex') }}</xf:cell>
						<xf:cell>{{ phrase('tag') }}</xf:cell>
						<xf:cell class="dataList-cell--min">{{ phrase('user') }}</xf:cell>
						<xf:cell class="dataList-cell--min">&nbsp;</xf:cell>
					</xf:datarow>
					
					<xf:foreach loop="$blacklistedTags" value="$blacklistedTag">
						<xf:datarow>
							<xf:toggle name="selected_tags[]" value="{$blacklistedTag.blacklist_id}" />
							<xf:cell>
								<xf:checkbox standalone="true">
								<xf:option value="1" selected="{$blacklistedTag.regex}" readonly="true" disabled="true"/>
								</xf:checkbox>
							</xf:cell>
							<xf:cell>{$blacklistedTag.tag}</xf:cell>
							<xf:cell>{$blacklistedTag.User.username}</xf:cell>
							<xf:delete href="{{ link('tags/delete-blacklist', null, {'blacklist_id': $blacklistedTag.blacklist_id}) }}" />
						</xf:datarow>
					</xf:foreach>
				</xf:datalist>
			</div>

			<div class="block-footer block-footer--split">
				<span class="block-footer-counter">{{ display_totals($blacklistedTags, $total) }}</span>

				<span class="block-footer-select">
					<xf:checkbox standalone="true">
						<xf:option data-match="input:checkbox[name='selected_tags[]']" check-all="< .block-container" label="{{ phrase('select_all') }}" />
					</xf:checkbox>
				</span>

				<span class="block-footer-controls">
					<xf:select name="state" class="input--inline" style="font-family: 'Font Awesome 5 Pro','FontAwesome';">
						<xf:option>{{ phrase('with_selected...') }}</xf:option>
						<!--[XF:action_menu:top]-->
						<xf:option value="delete">&#xf1f8; {{ phrase('delete') }}</xf:option>
						<!--[XF:action_menu:bottom]-->
					</xf:select>
					<xf:button type="submit">{{ phrase('go') }}</xf:button>
				</span>
			</div>
		<xf:else />
			<div class="block-body block-row">{{ phrase('avForumsTagEss_no_blacklisted_tags_found') }}</div>
		</xf:if>
	</div>
	
	<xf:pagenav page="{$page}" perpage="{$perPage}" total="{$total}"
				link="tags/blacklisted" params="{$filters}"
				wrapperclass="block-outer block-outer--after" />
</xf:form>]]></template>
