  <template type="admin" title="avForumsTagEss_category_list" version_id="2000000" version_string="2.0.0"><![CDATA[<xf:title>{{ phrase('avForumsTagEss_tag_categories') }}</xf:title>

<xf:pageaction>
	<xf:button href="{{ link('tags/add-category') }}" icon="add">{{ phrase('avForumsTagEss_add_category') }}</xf:button>
</xf:pageaction>

<xf:form action="{{ link('tags/categories') }}" class="block" ajax="true">
	<div class="block-container">
		<xf:if is="$tagCategories is not empty">
			<div class="block-body">
				<xf:datalist data-xf-init="responsive-data-list">
					<xf:datarow rowtype="header">
						<xf:cell>{{ phrase('avForumsTagEss_category_identifier') }}</xf:cell>
						<xf:cell>{{ phrase('title') }}</xf:cell>
						<xf:cell class="dataList-cell--min">&nbsp;</xf:cell>
					</xf:datarow>
					
					<xf:foreach loop="$tagCategories" key="$tagCategoryId" value="$tagCategoryTitle">
						<xf:datarow>
							<xf:cell href="{{ link('tags/edit-category', null, {'category_id': $tagCategoryId}) }}">{$tagCategoryId}</xf:cell>
							<xf:cell href="{{ link('tags/edit-category', null, {'category_id': $tagCategoryId}) }}">{$tagCategoryTitle}</xf:cell>
							<xf:delete href="{{ link('tags/delete-category', null, {'category_id': $tagCategoryId}) }}" />
						</xf:datarow>
					</xf:foreach>
				</xf:datalist>
			</div>
			
			<div class="block-footer">
				<span class="block-footer-counter">{{ display_totals($tagCategories, $total) }}</span>
			</div>
		<xf:else />
			<div class="block-body block-row">{{ phrase('avForumsTagEss_no_tag_categories_found') }}</div>
		</xf:if>
	</div>
</xf:form>]]></template>
