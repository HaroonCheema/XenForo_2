  <template type="public" title="fs_car_details_list_index" version_id="1000010" version_string="1.0.0 Alpha"><![CDATA[<xf:title> {{ phrase('fs_car_details_list_all') }} </xf:title>

<div class="block">

	<div class="block-container">

		<xf:macro name="car_details_filter" arg-conditions="{$conditions}" />

		<div class="block-body">

			<xf:datalist data-xf-init="responsive-data-list">
				<xf:if is="$data is not empty">

					<xf:macro name="record_table_list" arg-data="{$data}" />
					<xf:else />
					<div class="blockMessage">
						{{ phrase('no_items_have_been_created_yet') }}
