		</div>

	</div>
	<div class="block-outer block-outer--after">
		<xf:pagenav
					page="{$page}"
					perpage="{$perPage}"
					total="{$total}"
					link="cars-list"
					data="{$data}"
					params="{$conditions}"
					wrapperclass="block-outer-main"
					/>
