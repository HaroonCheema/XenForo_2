{
    "template": "member_view",
    "description": "fs_ban_user_hide_all_tab_panes",
    "execution_order": 10,
    "enabled": true,
    "action": "preg_replace",
    "find": "/<ul class=\"tabPanes js-memberTabPanes\">.*?<!--\\[XF:tab_panes:end]-->\n<\\/ul>/is",
