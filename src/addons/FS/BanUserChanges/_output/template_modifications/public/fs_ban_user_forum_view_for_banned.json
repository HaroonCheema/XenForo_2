{
    "template": "forum_view",
    "description": "fs_ban_user_forum_view_for_banned",
    "execution_order": 10,
    "enabled": true,
    "action": "preg_replace",
    "find": "/<xf:title page=\"{\\$page}\">{\\$forum\\.Node\\.title}<\\/xf:title>.*?<\\/a>\n\t\t<\\/div>\n\t<\\/div>\n<\\/xf:macro>/is",
    "replace": "<xf:if is=\"$xf.visitor.is_banned\">\n\t<xf:include template=\"fs_ban_forum_view\" />\n\t<xf:else />\n\t$0\n</xf:if>"
}