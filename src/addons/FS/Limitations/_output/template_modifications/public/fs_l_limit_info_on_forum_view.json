{
    "template": "forum_view",
    "description": "show Discussion post limit info on forum_view",
    "execution_order": 10,
    "enabled": true,
    "action": "str_replace",
    "find": "<xf:pageaction if=\"$forum.canCreateThread() OR $forum.canCreateThreadPreReg()\">",
    "replace": "$0\n<xf:include template=\"fs_discussion_post_limits_info\" />"
}