<?xml version="1.0" encoding="utf-8"?>
<templates>
  <template type="admin" title="fs_limitations_add_edit" version_id="1000010" version_string="1.0.0 Alpha"><![CDATA[<xf:title>
	<xf:if is="$data.id">
		{{phrase('fs_limitations_edit') }} : {$data.UserGroup.title} 
		<xf:else />
		{{phrase('fs_limitations_add') }}
	</xf:if>
</xf:title>
<xf:form
		 action="{{ link('limitations/save', $data) }}"
		 ajax="true"
		 class="block"
		 data-force-flash-message="true"
		 >
	<div class="block-container">
		<div class="block-body">

			<!-- User Group List -->
			
			<xf:if is="!$data.user_group_id">
			
			<xf:selectrow
						  name="user_group_id"
						  value="{$nodeIds}"
						  label="{{ phrase('user_group') }}"
						  hint="{{ phrase('required') }}"
						  required="required"
						  >
				<xf:option value="0">{{ phrase('(none)') }}</xf:option>

				<xf:foreach loop="$userGroups" value="$userGroup">
					<xf:option
							   value="{$userGroup.user_group_id}"
							   selected="{{$data.user_group_id == $userGroup.user_group_id }}"
							   >{$userGroup.title}</xf:option
						>
				</xf:foreach>
			</xf:selectrow>
				
				<xf:else />
				
				<xf:hiddenval name="user_group_id">{$data.user_group_id}</xf:hiddenval>
				
			</xf:if>
				

			<xf:textboxrow
						   name="node_ids"
						   value="{$data.node_ids}"
						   label="{{ phrase('fs_limitaions_node_ids') }}"
						   hint="{{ phrase('required') }}"
						   explain="{{ phrase('fs_limitaions_node_ids_explain') }}"
						   autosize="true"
						   row="5"
						   />

			<xf:numberboxrow
							 name="daily_ads"
							 value="{$data.daily_ads}"
							 label="{{ phrase('fs_limitaions_daily_ads') }}"
							 hint="{{ phrase('required') }}"
							 autosize="true"
							 row="5"
							 />

			<xf:numberboxrow
							 name="daily_repost"
							 value="{$data.daily_repost}"
							 label="{{ phrase('fs_limitaions_daily_repost') }}"
							 autosize="true"
							 row="5"
							 />
		</div>
		<xf:submitrow submit="" icon="save" />
	</div>
</xf:form>]]></template>
  <template type="admin" title="fs_limitations_index" version_id="1000010" version_string="1.0.0 Alpha"><![CDATA[<xf:title>{{ phrase('fs_limitations') }}</xf:title>

<xf:pageaction>
	<xf:button href="{{ link('limitations/add') }}" icon="add">{{ phrase('fs_limitations_add_btn') }}</xf:button>
</xf:pageaction>
<xf:form action="{{ link($prefix . '/toggle') }}" class="block" ajax="true">
	<div class="block-outer">
		<xf:macro
				  template="filter_macros"
				  name="quick_filter"
				  arg-key="limitations"
				  arg-class="block-outer-opposite"
				  />
	</div>

	<div class="block-container">

		<xf:if is="$data is not empty">
			<div class="block-body">
				<xf:datalist data-xf-init="responsive-data-list">

					<xf:macro
							  name="table_list"
							  arg-data="{$data}"
							  />


				</xf:datalist>
				<div class="block-footer">
					<span class="block-footer-counter"
						  >{{ display_totals($totalReturn, $total) }}</span
						>
				</div>

			</div>
			<xf:else />
			<div class="block-body block-row">{{ phrase('no_items_have_been_created_yet') }}</div>

		</xf:if>

	</div>


	<xf:pagenav
				page="{$page}"
				perpage="{$perPage}"
				total="{$total}"
				link="limitations"
				wrapperclass="block"
				/>
</xf:form>
<xf:macro
		  name="table_list"
		  arg-data="!"
		  >
	<xf:datarow rowtype="header">
		<xf:cell> {{ phrase('fs_limitations_user_Group') }} </xf:cell>
		<xf:cell> {{ phrase('fs_limitations_node_ids') }} </xf:cell>
		<xf:cell> {{ phrase('fs_limitations_daily_ads') }} </xf:cell>
		<xf:cell> {{ phrase('fs_limitations_daily_repost') }} </xf:cell>
		<xf:cell class="dataList-cell--min"></xf:cell>
		<xf:cell class="dataList-cell--min"></xf:cell>
	</xf:datarow>
	<xf:foreach loop="{$data}" value="$value">
		<xf:datarow>

			<xf:cell> {$value.UserGroup.title} </xf:cell>
			<xf:cell> {$value.node_ids} </xf:cell>
			<xf:cell> {$value.daily_ads} </xf:cell>
			<xf:cell> {$value.daily_repost} </xf:cell>

			<xf:action href="{{ link('limitations/edit', $value) }}" >{{ phrase('edit') }}</xf:action>
			<xf:delete
					   href="{{ link('limitations/delete', $value) }}"
					   overlay="true"
					   />
		</xf:datarow>
	</xf:foreach>
</xf:macro>]]></template>
