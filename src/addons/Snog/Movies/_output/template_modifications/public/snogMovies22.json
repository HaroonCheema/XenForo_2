{
    "template": "forum_post_thread",
    "description": "Changes page title to post movie",
    "execution_order": 10,
    "enabled": true,
    "action": "str_replace",
    "find": "<xf:title>{{ phrase('post_thread') }}</xf:title>",
    "replace": "<xf:if is=\"$forum.forum_type_id == 'snog_movies_movie' && !$xf.options.tmdbthreads_mix\">\n\t<xf:title>{{ phrase('snog_movies_post_button') }}</xf:title>\n<xf:else />\n$0\t\n</xf:if>\n"
}